ะก++ 32

#include <iostream>

using namespace std;

void su(int m)
{
    int b, c;
    int sum = 0;
    int n[m];
    for (int i = 0; i < m; i++)
    {
        cin >> n[i];
    }
    for (int i = m - 1; i > 0; i--)
    {
        if (n[i] > n[i - 1])
        {
            sum += 1;
            cout << i << endl;
        }
    }
    cout << sum;
}

int main()
{
    int x;
    cin >> x;
    su(x);
    return 0;
}
